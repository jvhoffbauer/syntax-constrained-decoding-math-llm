{
    "api_list": [
        {
            "name": "Get Channel Points Context",
            "schema": {
                "community": {
                    "channel": {
                        "communityPointsSettings": {
                            "automaticRewards": [
                                {
                                    "cost": "str",
                                    "defaultCost": "str",
                                    "defaultImage": {
                                        "url": "str",
                                        "url2x": "str",
                                        "url4x": "str"
                                    },
                                    "globallyUpdatedForIndicatorAt": "str",
                                    "id": "str",
                                    "type": "str"
                                }
                            ],
                            "customRewards": [
                                {
                                    "cost": "str",
                                    "defaultImage": {
                                        "url": "str",
                                        "url2x": "str",
                                        "url4x": "str"
                                    },
                                    "globalCooldownSetting": {
                                        "globalCooldownSeconds": "str",
                                        "isEnabled": "str"
                                    },
                                    "id": "str",
                                    "isEnabled": "str",
                                    "isSubOnly": "str"
                                }
                            ]
                        }
                    }
                }
            }
        },
        {
            "name": "Get Stream Views",
            "schema": {
                "user": {
                    "id": "str",
                    "stream": "NoneType"
                }
            }
        },
        {
            "name": "Get Streamer Info",
            "schema": {
                "user": {
                    "id": "str",
                    "login": "str"
                }
            }
        },
        {
            "name": "Get Channel Panels",
            "schema": {
                "message": "str"
            }
        },
        {
            "name": "Get Stream Tags",
            "schema": {
                "user": {
                    "id": "str"
                }
            }
        },
        {
            "name": "Get Channel Goals",
            "schema": {
                "channel": {
                    "goals": {
                        "edges": [
                            {
                                "node": {
                                    "contributionType": "str",
                                    "createdAt": "str",
                                    "currentContributions": "int",
                                    "customizations": {
                                        "progressBarAccentColor": "str",
                                        "progressBarBackgroundColor": "str"
                                    },
                                    "description": "str",
                                    "id": "str",
                                    "state": "str",
                                    "targetContributions": "int"
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "name": "Get Pinned Chat",
            "schema": {
                "channel": {
                    "id": "str",
                    "pinnedChatMessages": {
                        "edges": "str",
                        "pageInfo": {
                            "hasNextPage": "bool"
                        }
                    }
                }
            }
        },
        {
            "name": "Get Chat Restrictions",
            "schema": {
                "message": "str"
            }
        }
    ]
}
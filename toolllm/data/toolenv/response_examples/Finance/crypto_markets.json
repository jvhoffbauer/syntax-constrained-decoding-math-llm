{
    "api_list": [
        {
            "name": "Exchange Information",
            "schema": {
                "timezone": "str",
                "serverTime": "int",
                "rateLimits": [
                    {
                        "rateLimitType": "str",
                        "interval": "str",
                        "intervalNum": "int",
                        "limit": "int"
                    }
                ],
                "exchangeFilters": "str",
                "symbols": [
                    {
                        "symbol": "str",
                        "status": "str",
                        "baseAsset": "str",
                        "baseAssetPrecision": "int",
                        "quoteAsset": "str",
                        "quotePrecision": "int",
                        "quoteAssetPrecision": "int",
                        "orderTypes": [
                            "list of str with length 1"
                        ],
                        "icebergAllowed": "bool",
                        "ocoAllowed": "bool",
                        "quoteOrderQtyMarketAllowed": "bool",
                        "allowTrailingStop": "bool",
                        "cancelReplaceAllowed": "bool",
                        "isSpotTradingAllowed": "bool",
                        "isMarginTradingAllowed": "bool",
                        "filters": [
                            {
                                "filterType": "str",
                                "minPrice": "str",
                                "maxPrice": "str",
                                "tickSize": "str"
                            }
                        ],
                        "permissions": [
                            "list of str with length 1"
                        ]
                    }
                ]
            }
        },
        {
            "name": "Order Book",
            "schema": {
                "lastUpdateId": "int",
                "bids": [
                    "list of str with length 1"
                ],
                "asks": [
                    "list of str with length 1"
                ]
            }
        },
        {
            "name": "Recent Trades List",
            "schema": {
                "price": "str",
                "qty": "str",
                "quoteQty": "str",
                "isBuyerMaker": "bool",
                "isBestMatch": "bool"
            }
        },
        {
            "name": "Symbol Order Book Ticker",
            "schema": {
                "symbol": "str",
                "bidPrice": "str",
                "askPrice": "str"
            }
        }
    ]
}
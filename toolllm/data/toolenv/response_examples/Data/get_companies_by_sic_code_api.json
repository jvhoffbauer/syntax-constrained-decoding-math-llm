{
    "api_list": [
        {
            "name": "Search Companies by Name",
            "schema": {
                "results": {
                    "companies": [
                        {
                            "company_name": "str",
                            "company_name_slug": "str",
                            "company_city_state": "str"
                        }
                    ]
                },
                "total": "int"
            }
        },
        {
            "name": "Get Company Data by Company Slug",
            "schema": {
                "result": {
                    "company_name": "str",
                    "company_slug": "str",
                    "company_sic": "int",
                    "company_naics": "int",
                    "overview": {
                        "City": "str",
                        "State": "str",
                        "ZIP": "str",
                        "Country": "str",
                        "Est. Annual Revenue": "str",
                        "Years In Business": "str",
                        "Est. Company Size": "str"
                    }
                },
                "status": "str"
            }
        },
        {
            "name": "Get Companies by SIC Code",
            "schema": {
                "sic_code": "int",
                "naics_code": "int",
                "results": {
                    "details": {
                        "sic_details": "str",
                        "naics_detail": "str"
                    },
                    "companies": [
                        {
                            "company_name": "str",
                            "company_name_slug": "str",
                            "company_city_state": "str"
                        }
                    ]
                },
                "total": "int",
                "status": "str"
            }
        }
    ]
}
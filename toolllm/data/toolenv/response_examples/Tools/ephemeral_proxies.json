{
    "api_list": [
        {
            "name": "Check datacenter proxies service status",
            "schema": {
                "success": "bool",
                "service_status": {
                    "availability": {
                        "total": {
                            "proxies": "int"
                        },
                        "by_country": [
                            {
                                "country_iso": "str",
                                "proxies": "int"
                            }
                        ]
                    }
                }
            }
        },
        {
            "name": "Obtain a new datacenter proxy",
            "schema": {
                "success": "bool",
                "proxy": {
                    "host": "str",
                    "port": "int",
                    "expires_at": "str",
                    "whitelisted_ips": [
                        "list of str with length 1"
                    ],
                    "features": {
                        "static": "bool",
                        "type": "str",
                        "supported_protocols": {
                            "http": "bool",
                            "https": "bool"
                        }
                    },
                    "visibility": {
                        "ip": "str",
                        "country": "str",
                        "country_iso": "str",
                        "latitude": "float",
                        "longitude": "float",
                        "timezone": "str",
                        "asn": "str",
                        "asn_org": "str",
                        "zip_code": "str",
                        "region_name": "str",
                        "region_code": "str",
                        "city": "str"
                    }
                }
            }
        },
        {
            "name": "Get user's traffic balance",
            "schema": {
                "success": "bool",
                "balance": {
                    "consumed_megabytes": "int",
                    "limit_megabytes": "int"
                }
            }
        },
        {
            "name": "Obtain a new residential proxy",
            "schema": {
                "success": "bool",
                "message": "str"
            }
        },
        {
            "name": "Check residential proxies service status",
            "schema": {
                "success": "bool",
                "service_status": {
                    "availability": {
                        "total": {
                            "proxies": "int"
                        },
                        "by_country": [
                            {
                                "country_iso": "str",
                                "proxies": "int"
                            }
                        ]
                    }
                }
            }
        }
    ]
}
{
    "api_list": [
        {
            "name": "Get Language Code",
            "schema": {
                "data": [
                    {
                        "code": "str",
                        "language": "str"
                    }
                ]
            }
        },
        {
            "name": "Translation",
            "schema": {
                "data": {
                    "fromLanguage": "str",
                    "toLanguage": "str",
                    "translatedText": "str",
                    "translationEngine": "str"
                }
            }
        },
        {
            "name": "Correct and Rephrase",
            "schema": {
                "status": "bool",
                "message": "str",
                "data": {
                    "language": "str",
                    "text": "str",
                    "engine": "str",
                    "corrections": [
                        {
                            "group": "str",
                            "type": "str",
                            "shortDescription": "str",
                            "correctionText": "str"
                        }
                    ],
                    "sentences": [
                        {
                            "status": "str"
                        }
                    ],
                    "stats": {
                        "textLength": "int",
                        "wordCount": "int",
                        "sentenceCount": "int",
                        "longestSentence": "int"
                    }
                }
            }
        },
        {
            "name": "Get Definitions",
            "schema": {
                "data": {
                    "DefsByWord": [
                        {
                            "DefinedWord": "str",
                            "DefsByPos": [
                                {
                                    "Defs": [
                                        {
                                            "Def": "str",
                                            "HighFreq": "bool"
                                        }
                                    ],
                                    "Pos": "str"
                                }
                            ],
                            "SeeAlso": [
                                {
                                    "Word": "str"
                                }
                            ]
                        }
                    ],
                    "OrigWord": "str",
                    "PosInContext": "str"
                }
            }
        },
        {
            "name": "Correction In Sentence",
            "schema": {
                "status": "bool",
                "message": "str",
                "data": {
                    "Corrections": [
                        {
                            "Confidence": "int",
                            "From": "int",
                            "LrnFrg": "str",
                            "ShouldReplace": "bool",
                            "Suggestions": [
                                {
                                    "Text": "str"
                                }
                            ],
                            "To": "int",
                            "Type": "int"
                        }
                    ],
                    "Sentences": [
                        {
                            "IsEnglish": "bool",
                            "ExceededCharacterLimit": "bool"
                        }
                    ]
                }
            }
        },
        {
            "name": "Rephrase",
            "schema": {
                "status": "bool",
                "data": {
                    "Sentences": [
                        {
                            "Sentence": "str"
                        }
                    ]
                }
            }
        },
        {
            "name": "Get Synonyms",
            "schema": {
                "data": {
                    "search": "str",
                    "language": "str",
                    "allowredirect": "str",
                    "rude": "str",
                    "colloquial": "str",
                    "resultsCount": "str",
                    "results": [
                        {
                            "pos": {
                                "mask": "str"
                            },
                            "weight": "str",
                            "relevance": "str",
                            "rudeResults": "str",
                            "colloquialResults": "str",
                            "relevantitems": "str",
                            "cluster": [
                                {
                                    "word": "str",
                                    "language": "str",
                                    "cluster": "str",
                                    "weight": "str",
                                    "rude": "str",
                                    "colloquial": "str",
                                    "relevance": "str"
                                }
                            ],
                            "examples": [
                                {
                                    "cluster": "str",
                                    "example": "str",
                                    "pos": {
                                        "mask": "str"
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        }
    ]
}
{
    "api_list": [
        {
            "name": "Predictions",
            "schema": {
                "data": [
                    {
                        "home_team": "str",
                        "away_team": "str",
                        "market": "str",
                        "competition_name": "str",
                        "prediction": "str",
                        "competition_cluster": "str",
                        "status": "str",
                        "federation": "str",
                        "season": "str",
                        "result": "str",
                        "start_date": "str",
                        "odds": {
                            "1": "float",
                            "X": "float",
                            "2": "float",
                            "1X": "float",
                            "X2": "float",
                            "12": "float"
                        }
                    }
                ]
            }
        },
        {
            "name": "List available markets",
            "schema": {
                "data": {
                    "allowed_for_your_subscription": "empty list",
                    "all": "empty list"
                }
            }
        },
        {
            "name": "Head To Head",
            "schema": {
                "data": {
                    "stats": {
                        "overall": {
                            "num_encounters": "str",
                            "over_05": "str",
                            "over_15": "str",
                            "over_25": "str",
                            "over_35": "str",
                            "both_teams_scored": "str",
                            "total_goals": "str",
                            "avg_goals_per_match": "str"
                        },
                        "home_team": {
                            "team_name": "str",
                            "goals_scored": "str",
                            "goals_conceived": "str",
                            "won": "str",
                            "draw": "str",
                            "lost": "str",
                            "clean_sheet": "str",
                            "first_half_win": "str",
                            "first_half_draw": "str",
                            "first_half_lost": "str",
                            "avg_bookie_win_chance": "str",
                            "avg_bookie_lose_chance": "str"
                        },
                        "away_team": {
                            "team_name": "str",
                            "goals_scored": "str",
                            "goals_conceived": "str",
                            "won": "str",
                            "draw": "str",
                            "lost": "str",
                            "clean_sheet": "str",
                            "first_half_win": "str",
                            "first_half_draw": "str",
                            "first_half_lost": "str",
                            "avg_bookie_win_chance": "str",
                            "avg_bookie_lose_chance": "str"
                        }
                    }
                }
            }
        },
        {
            "name": "Prediction Details",
            "schema": {
                "data": [
                    {
                        "id": "int",
                        "competition_name": "str",
                        "home_team": "str",
                        "away_team": "str",
                        "result": "str",
                        "last_update_at": "str",
                        "available_markets": [
                            "list of str with length 1"
                        ],
                        "prediction_per_market": {
                            "classic": {
                                "status": "str",
                                "prediction": "str",
                                "odds": {
                                    "1": "float",
                                    "X": "float",
                                    "2": "float",
                                    "1X": "float",
                                    "X2": "float",
                                    "12": "float"
                                }
                            }
                        }
                    }
                ]
            }
        },
        {
            "name": "List available federations",
            "schema": {
                "data": [
                    "list of str with length 1"
                ]
            }
        },
        {
            "name": "Performance stats for past predictions",
            "schema": {
                "data": {
                    "accuracy": {
                        "last_30_days": "float",
                        "yesterday": "float",
                        "last_7_days": "float",
                        "last_14_days": "float"
                    }
                }
            }
        },
        {
            "name": "Get list of fixture IDs",
            "schema": {
                "data": "empty list"
            }
        }
    ]
}
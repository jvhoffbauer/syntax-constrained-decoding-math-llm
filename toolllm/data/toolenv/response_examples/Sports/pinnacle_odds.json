{
    "api_list": [
        {
            "name": "List of periods",
            "schema": {
                "periods": [
                    {
                        "number": "int",
                        "description": "str",
                        "spread_description": "str",
                        "moneyline_description": "str",
                        "total_description": "str"
                    }
                ]
            }
        },
        {
            "name": "List of archive events",
            "schema": {
                "detail": [
                    {
                        "msg": "str",
                        "type": "str"
                    }
                ]
            }
        },
        {
            "name": "List of sports",
            "schema": {
                "name": "str",
                "last": "int",
                "special_last": "int"
            }
        },
        {
            "name": "List of leagues",
            "schema": {
                "leagues": [
                    {
                        "id": "int",
                        "name": "str",
                        "homeTeamType": "str",
                        "hasOfferings": "bool",
                        "allowRoundRobins": "bool"
                    }
                ]
            }
        }
    ]
}
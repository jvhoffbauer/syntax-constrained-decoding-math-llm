{
    "api_list": [
        {
            "name": "Get radio playlist",
            "schema": {
                "total": "int",
                "mediaItems": [
                    {
                        "uri": "str"
                    }
                ]
            }
        },
        {
            "name": "User followers",
            "schema": {
                "profiles": [
                    {
                        "name": "str",
                        "followers_count": "int"
                    }
                ]
            }
        },
        {
            "name": "User profile",
            "schema": {
                "uri": "str",
                "name": "str",
                "image_url": "str",
                "followers_count": "int",
                "public_playlists": [
                    {
                        "uri": "str",
                        "name": "str",
                        "image_url": "str",
                        "followers_count": "int"
                    }
                ],
                "is_verified": "bool",
                "report_abuse_disabled": "bool",
                "has_spotify_name": "bool",
                "has_spotify_image": "bool",
                "allow_follows": "bool",
                "show_follows": "bool"
            }
        },
        {
            "name": "Get playlist",
            "schema": {
                "collaborative": "bool",
                "description": "str",
                "followers": {
                    "total": "int"
                },
                "images": [
                    {
                        "url": "str"
                    }
                ],
                "name": "str",
                "owner": {
                    "display_name": "str",
                    "id": "str"
                },
                "public": "bool",
                "tracks": {
                    "total": "int"
                },
                "uri": "str"
            }
        },
        {
            "name": "Track lyrics",
            "schema": {
                "lyrics": {
                    "syncType": "str",
                    "lines": [
                        {
                            "words": "str"
                        }
                    ],
                    "providerDisplayName": "str",
                    "language": "str"
                }
            }
        },
        {
            "name": "Track credits",
            "schema": {
                "trackUri": "str",
                "trackTitle": "str",
                "roleCredits": [
                    {
                        "roleTitle": "str",
                        "artists": [
                            {
                                "name": "str"
                            }
                        ]
                    }
                ],
                "sourceNames": [
                    "list of str with length 1"
                ]
            }
        },
        {
            "name": "Get tracks",
            "schema": {
                "tracks": [
                    {
                        "album": {
                            "album_type": "str",
                            "artists": [
                                {
                                    "name": "str"
                                }
                            ],
                            "name": "str",
                            "release_date": "str",
                            "total_tracks": "str"
                        },
                        "artists": [
                            {
                                "name": "str"
                            }
                        ],
                        "disc_number": "str",
                        "duration_ms": "str",
                        "explicit": "str",
                        "external_ids": {
                            "isrc": "str"
                        },
                        "id": "str",
                        "name": "str",
                        "popularity": "str",
                        "track_number": "str"
                    }
                ]
            }
        },
        {
            "name": "Artist related",
            "schema": {
                "artists": [
                    {
                        "external_urls": {
                            "spotify": "str"
                        },
                        "genres": [
                            "list of str with length 1"
                        ],
                        "id": "str",
                        "name": "str",
                        "popularity": "int",
                        "type": "str",
                        "uri": "str"
                    }
                ]
            }
        },
        {
            "name": "Artist featuring",
            "schema": {
                "data": {
                    "artist": {
                        "id": "str",
                        "uri": "str",
                        "profile": {
                            "name": "str"
                        },
                        "relatedContent": {
                            "featuring": {
                                "totalCount": "int"
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "Artist discovered on",
            "schema": {
                "data": {
                    "artist": {
                        "id": "str",
                        "uri": "str",
                        "profile": {
                            "name": "str"
                        },
                        "relatedContent": {
                            "discoveredOn": {
                                "totalCount": "int",
                                "items": [
                                    {
                                        "uri": "str",
                                        "owner": {
                                            "name": "str"
                                        },
                                        "name": "str",
                                        "description": "str",
                                        "images": {
                                            "totalCount": "int"
                                        }
                                    }
                                ]
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "Artist singles",
            "schema": {
                "data": {
                    "artist": {
                        "discography": {
                            "singles": {
                                "totalCount": "int",
                                "items": [
                                    {
                                        "releases": {
                                            "items": [
                                                {
                                                    "id": "str",
                                                    "uri": "str",
                                                    "name": "str",
                                                    "type": "str",
                                                    "date": {
                                                        "year": "int",
                                                        "isoString": "str"
                                                    },
                                                    "coverArt": {
                                                        "sources": [
                                                            {
                                                                "url": "str",
                                                                "width": "int",
                                                                "height": "int"
                                                            }
                                                        ]
                                                    },
                                                    "playability": {
                                                        "playable": "bool"
                                                    },
                                                    "tracks": {
                                                        "totalCount": "int"
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "Artist albums",
            "schema": {
                "data": {
                    "artist": {
                        "discography": {
                            "albums": {
                                "items": [
                                    {
                                        "releases": {
                                            "items": [
                                                {
                                                    "id": "str",
                                                    "uri": "str",
                                                    "name": "str",
                                                    "date": {
                                                        "year": "int",
                                                        "isoString": "str"
                                                    },
                                                    "coverArt": {
                                                        "sources": [
                                                            {
                                                                "url": "str"
                                                            }
                                                        ]
                                                    },
                                                    "tracks": {
                                                        "totalCount": "int"
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "Artist discography overview",
            "schema": {
                "data": {
                    "artist": {
                        "id": "str",
                        "uri": "str",
                        "profile": {
                            "name": "str"
                        },
                        "discography": {
                            "albums": {
                                "totalCount": "int"
                            },
                            "singles": {
                                "totalCount": "int"
                            },
                            "compilations": {
                                "totalCount": "int"
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "Get artists",
            "schema": {
                "artists": [
                    {
                        "external_urls": {
                            "spotify": "str"
                        },
                        "id": "str",
                        "name": "str",
                        "popularity": "int",
                        "type": "str",
                        "uri": "str"
                    }
                ]
            }
        },
        {
            "name": "Album tracks",
            "schema": {
                "data": {
                    "album": {
                        "tracks": {
                            "totalCount": "int",
                            "items": [
                                {
                                    "track": {
                                        "name": "str",
                                        "duration": {
                                            "totalMilliseconds": "int"
                                        },
                                        "playability": {
                                            "playable": "bool"
                                        },
                                        "artists": {
                                            "items": [
                                                {
                                                    "profile": {
                                                        "name": "str"
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        {
            "name": "Get albums",
            "schema": {
                "albums": [
                    {
                        "album_type": "str",
                        "artists": [
                            {
                                "name": "str"
                            }
                        ],
                        "id": "str",
                        "name": "str",
                        "images": [
                            {
                                "height": "str",
                                "url": "str",
                                "width": "str"
                            }
                        ],
                        "label": "str",
                        "release_date": "str",
                        "total_tracks": "str",
                        "tracks": {
                            "items": [
                                {
                                    "artists": [
                                        {
                                            "name": "str"
                                        }
                                    ],
                                    "name": "str",
                                    "track_number": "str"
                                }
                            ],
                            "total": "str"
                        }
                    }
                ]
            }
        },
        {
            "name": "Search",
            "schema": {
                "albums": {
                    "totalCount": "int"
                },
                "artists": {
                    "totalCount": "int"
                },
                "episodes": {
                    "totalCount": "int"
                },
                "genres": {
                    "totalCount": "int"
                },
                "playlists": {
                    "totalCount": "int"
                },
                "podcasts": {
                    "totalCount": "int"
                },
                "topResults": {},
                "tracks": {
                    "totalCount": "int"
                },
                "users": {
                    "totalCount": "int"
                }
            }
        }
    ]
}
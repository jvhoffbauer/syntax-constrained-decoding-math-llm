{
    "api_list": [
        {
            "name": "/network/management/remotes",
            "schema": {
                "active": "str",
                "link_enable": "bool",
                "realtime_logging": "bool",
                "heartbeat": "bool"
            }
        },
        {
            "name": "/network/timezone",
            "schema": {
                "current_timezone": "str"
            }
        },
        {
            "name": "/api/info",
            "schema": {
                "firmware_version": "str",
                "framework_version": "str",
                "api_version": "str",
                "device_name": "str",
                "device_type": "str",
                "device_serial_number": "str",
                "hybrid_enable": "bool"
            }
        },
        {
            "name": "/desktop/messages/items",
            "schema": {
                "message": "str",
                "date": "str"
            }
        },
        {
            "name": "/device/mode/current",
            "schema": {
                "current_panel_mode": "str",
                "no_unit": "bool"
            }
        },
        {
            "name": "/dashboard",
            "schema": {
                "framework_version": "str",
                "launcher_version": "str",
                "hybrid_enable": "bool"
            }
        },
        {
            "name": "/network/settings",
            "schema": {
                "dhcp": "bool",
                "ip_address": "str",
                "mask": "str",
                "gateway": "str"
            }
        },
        {
            "name": "/forward/legacy/items/list",
            "schema": {
                "count": "int",
                "list": [
                    {
                        "apartment_id": "int",
                        "forward_entity_list": [
                            {
                                "forward_entity": "str"
                            }
                        ]
                    }
                ]
            }
        },
        {
            "name": "/access/general/remote/control/settings",
            "schema": {
                "enable": "bool",
                "custom_server_enabled": "bool"
            }
        },
        {
            "name": "/access/timeprofile/item/{timeProfileUid}",
            "schema": {
                "name": "str",
                "time_options": {
                    "is_all_day": "bool",
                    "date_from": "int",
                    "date_to": "int"
                },
                "repeat_options": {
                    "repeat_enable": "bool",
                    "repeat_format": {
                        "repeat_type": "str",
                        "repeat_end": {
                            "format": "str",
                            "date": "int"
                        }
                    }
                },
                "identifiers": {
                    "count": "int"
                },
                "link_id": "str"
            }
        },
        {
            "name": "/device/language",
            "schema": {
                "current_language": "str"
            }
        },
        {
            "name": "/advanced/announcement",
            "schema": {
                "announcement_enable": "bool",
                "announcement_url": "str"
            }
        },
        {
            "name": "/advanced/rtsp/feed",
            "schema": {
                "rtsp_feed_url_list": [
                    {
                        "rtsp_url": "str"
                    }
                ]
            }
        },
        {
            "name": "/log/list",
            "schema": {
                "list_option": [
                    {
                        "locale": {
                            "available_locales": [
                                "list of str with length 1"
                            ]
                        },
                        "filter": {
                            "available_filtering": "bool",
                            "available_fields": [
                                {
                                    "filter_field": "str",
                                    "available_types": [
                                        "list of str with length 1"
                                    ]
                                }
                            ],
                            "available_values": {
                                "category": [
                                    "list of str with length 1"
                                ],
                                "priority": [
                                    "list of str with length 1"
                                ],
                                "name": [
                                    {
                                        "key": "str",
                                        "localized": "str",
                                        "english": "str"
                                    }
                                ]
                            }
                        },
                        "sort": {
                            "asc": "bool",
                            "available_sorting": [
                                "list of str with length 1"
                            ]
                        }
                    }
                ],
                "list_items": [
                    {
                        "timestamp": "int",
                        "category": "str",
                        "priority": "str",
                        "name": {
                            "key": "str",
                            "localized": "str",
                            "english": "str"
                        }
                    }
                ]
            }
        },
        {
            "name": "/system/firmware/server/custom",
            "schema": {
                "is_custom_server_enable": "bool",
                "custom_server_url": "str"
            }
        }
    ]
}
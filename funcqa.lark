?start: functioncall
    ?functioncall: "<T>" name "(" args ")" "</T>"
    name: "multiply" | "add" | "subtract" | "divide"
    args: (value "," value)
    value: INT+

    %import common.STRING
    %import common.SIGNED_NUMBER
    %import common.DIGIT
    %import common.INT
    %import common.WS
    %ignore WS
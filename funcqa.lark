?start: functioncall
    ?functioncall: "<T>" name "(" args ")"
    name: "multiply" | "add" | "subtract" | "divide"
    args: arg ("," arg)*
    arg: value | functioncall
    value: INT+ | INT+ "." INT+

    %import common.STRING
    %import common.SIGNED_NUMBER
    %import common.DIGIT
    %import common.INT
    %import common.WS
    %ignore WS